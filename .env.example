# Groq API Configuration
GROQ_API_KEY=YOUR_GROQ_API_KEY
GROQ_FAST_MODEL=openai/gpt-oss-20b
GROQ_DEFAULT_TEMPERATURE=0.1
GROQ_REASONING_MODEL=openai/gpt-oss-120b
GROQ_FAST_VISION_TO_TEXT_MODEL=meta-llama/llama-4-scout-17b-16e-instruct
GROQ_RESONING_VISION_TO_TEXT_MODEL=meta-llama/llama-4-maverick-17b-128e-instruct

# langsmith key
LANGSMITH_TRACING=true
LANGSMITH_ENDPOINT=https://api.smith.langchain.com
LANGSMITH_API_KEY=YOUR_LANGSMITH_API_KEY
LANGSMITH_PROJECT=ai-powered-etl-process

# SUPABASE POSTGRESSSQL
SUPABASE_URL=https://<your-project-ref>.supabase.co
SUPABASE_KEY=YOUR_SUPABASE_ANON_OR_SERVICE_ROLE_KEY
SUPABASE_PASSWORD=YOUR_SUPABASE_DB_PASSWORD

# PostgreSQL Connection String (Supabase) - for Receipt Data
# Direct connection (port 5432) - not pooler
DATABASE_URL=postgresql://postgres:YOUR_SUPABASE_DB_PASSWORD@db.<your-project-ref>.supabase.co:5432/postgres